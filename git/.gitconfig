[user]
	email = 903716+akrawchyk@users.noreply.github.com
	name = Andrew Krawchyk
[apply]
	whitespace = fix
[core]
	autocrlf = input
	whitespace = trailing-space,space-before-tab
	safecrlf = true
	trustctime = false
	excludesfile = ~/.gitignore_global
[push]
	default = current
[diff]
	renames = copies
	tool = vimdiff
[help]
	autocorrect = 1
[merge]
	log = true
	tool = fugitive
[mergetool "fugitive"]
	cmd = vim -f -c \"Gdiff\" \"$MERGED\" -c \"wincmd =\"
	trustExitCode = false
[credential]
	helper = osxkeychain
[color "diff"]
	whitespace = red reverse
[color]
	ui = true
