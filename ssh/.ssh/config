# Ensure KnownHosts are unreadable if leaked - it is otherwise easier to know
# which hosts your keys have access to.
HashKnownHosts yes

Host bandit
	HostName bandit.labs.overthewire.org

Host hg.mozilla.org
	User akrawchyk@gmail.com
	IdentityFile ~/.ssh/id_rsa_mozilla_2015-10-20

Host reviewboard-hg.mozilla.org
	User akrawchyk@gmail.com
	IdentityFile ~/.ssh/id_rsa_mozilla_2015-10-20

Host heroku.com
	HostName heroku.com
	IdentityFile /Users/andrew/.ssh/id_rsa_heroku_2016-01-21
	IdentitiesOnly yes
